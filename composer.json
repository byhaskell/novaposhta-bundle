{
  "name": "byhaskell/novaposhta-bundle",
  "description": "Nova Poshta integration bundle for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": { "byhaskell\\NovaPoshtaBundle\\": "src/" }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/http-client-contracts": "^3.0"
  },
  "require-dev": {
    "symfony/http-client": "^6.4 || ^7.0"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
